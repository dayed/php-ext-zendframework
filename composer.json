{
    "name": "golovanov/php-ext-zendframework",
    "description": "Zend Framework 2 delivered as a C-extension (Zephir)",
    "keywords": ["zendframework", "zf2", "extension", "zend", "zephir"],
    "homepage": "https://github.com/golovanov/php-ext-zendframework/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/golovanov/php-ext-zendframework/issues?state=open"
    },
    "require": {
        "phalcon/zephir": "*",
        "zendframework/zendframework": "2.3.3"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "2.*",
        "phpunit/phpunit": "*",
        "doctrine/annotations": ">=1.0",
        "ircmaxell/random-lib": "dev-master",
        "ircmaxell/security-lib": "dev-master",
        "mikey179/vfsStream": "1.2.*",
        "fabpot/php-cs-fixer": "dev-master",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpcov": "*"
    },
    "autoload": {
        "psr-4": {
            "ZephirExtra\\": "utils/zephir-extra"
        }
    },
    "authors": [
        {
            "name": "Igor Golovanov",
            "email": "igor.golovanov@gmail.com"
        }
    ],
    "replace": {
        "zendframework/zend-stdlib": "self.version"
    }
}
